#:kivy 1.0.9

<CameraBox>:
    orientation: 'vertical'
    button: button1
    img1: image1
    Image:
        id: image1
        center_x: root.center_x
        center_y: root.center_y
    ToggleButton:
        id: button1
        text: 'Change Color'
        size_hint_y: None
        on_press: root.change_color()
        height: '48dp'